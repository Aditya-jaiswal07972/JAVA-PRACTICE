import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc = new Scanner(System.in);
        double a = sc.nextDouble();
        double b = sc.nextDouble();
        if(a>0 && b>0){
            System.out.print(a+"-"+b);
        }else{
             System.out.print("Invalid Input");
        }
    }
}
